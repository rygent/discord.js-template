    "dotenv": "^16.0.3",
    "globby": "^13.1.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.46",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "commander": "^10.0.0",
    "eslint": "^8.37.0",
