{
  "name": "discord.js-template",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext mjs,js,ts --fix",
    "build": "rimraf ./dist && swc ./src --out-dir ./dist --copy-files",
    "start": "node --no-warnings ./dist/index.js",
    "register": "node ./dist/interactions/register.js",
    "update": "yarn upgrade-interactive"
  },
  "license": "GPL-3.0",
  "private": true,
  "dependencies": {
    "colorette": "^2.0.20",
    "discord.js": "^14.9.0",
    "dotenv": "^16.0.3",
    "globby": "^13.1.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.57",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "commander": "^10.0.1",
    "eslint": "^8.40.0",
    "rimraf": "^4.4.1",
    "semver": "^7.5.0",
    "typescript": "^5.0.4"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^6.0.3"
  },
  "packageManager": "yarn@3.5.0"
