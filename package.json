{
  "name": "discord.js-template",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext mjs,js,ts --fix",
    "format": "prettier --write .",
    "build": "rimraf ./dist && swc ./src --out-dir ./dist --copy-files",
    "start": "node --no-warnings ./dist/index.js",
    "register": "node ./dist/interactions/register.js",
    "update": "yarn upgrade-interactive"
  },
  "license": "GPL-3.0",
  "private": true,
  "dependencies": {
    "colorette": "^2.0.20",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "globby": "^13.2.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.67",
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "commander": "^10.0.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "semver": "^7.5.3",
    "typescript": "^5.1.6"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^6.0.3"
  },
  "packageManager": "yarn@3.6.1"
}
